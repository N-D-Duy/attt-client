<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.duynguyen.atttclient.presentation.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label fx:id="userInfoLabel" text="Online Users"/>
    <ListView fx:id="onlineUsersListView" prefHeight="200.0" prefWidth="200.0" onMouseClicked="#onSendFileButtonClick"/>
</VBox>
